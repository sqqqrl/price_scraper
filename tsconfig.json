{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "module": "commonjs",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "pretty": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "rootDir": ".",
    "outDir": "lib",
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "dump"
  ],
  "include": ["src/**/*.ts", "__tests__/**/*.ts"],
  "skipLibCheck": true,
}